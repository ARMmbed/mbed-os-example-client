{
    "config": {
        "button1": {
            "help": "Use BUTTON1 from PinNames.h by default",
            "value": "BUTTON1"
        }
    },
    "macros": ["MBEDTLS_USER_CONFIG_FILE=\"mbedtls_mbed_client_config.h\"", "MBED_CONF_RSA_REQUIRED"],
    "target_overrides": {
        "*": {
            "target.features_add": ["LWIP", "COMMON_PAL"],
            "platform.stdio-baud-rate": 115200,
            "platform.stdio-convert-newlines":true,
            "lwip.ipv4-enabled": true,
            "lwip.ipv6-enabled": false,
            "mbed-trace.enable": 0,
            "easy-connect.wifi-type": "\"ODIN\"",
            "easy-connect.network-interface": "WIFI_ODIN",
            "easy-connect.network-interface-header": "\"OdinWiFiInterface.h\"",
            "easy-connect.network-interface-class": "OdinWiFiInterface",
            "easy-connect.network-interface-name": "\"OdinWiFiInterface\"",
            "easy-connect.wifi-ssid": "\"SSID\"",
            "easy-connect.wifi-password": "\"Password\""
        },
        "NUCLEO_F401RE": {
            "wifi-tx": "PA_11",
            "wifi-rx": "PA_12"
        },
        "NUCLEO_F411RE": {
            "wifi-tx": "PA_11",
            "wifi-rx": "PA_12"
        }
    }
}
