{
    "config": {
        "wifi-tx": {
            "help": "TX pin for serial connection to external device",
            "value": "D8"
        },
        "wifi-rx": {
            "help": "RX pin for serial connection to external device",
            "value": "D2"
        }
    },
    "macros": ["MBEDTLS_USER_CONFIG_FILE=\"mbedtls_mbed_client_config.h\"", "IDW04A1_WIFI_HW_BUG_WA"],
    "target_overrides": {
        "*": {
            "target.features_add": ["COMMON_PAL"],
            "platform.stdio-baud-rate": 115200,
            "platform.stdio-convert-newlines": true,
            "mbed-trace.enable": 0,
            "idw0xx1.expansion-board": "IDW04A1",
            "drivers.uart-serial-txbuf-size": 512,
            "drivers.uart-serial-rxbuf-size": 512,
            "easy-connect.wifi-type": "\"IDW0XX1\"",
            "easy-connect.network-interface": "WIFI_IDW0XX1",
            "easy-connect.network-interface-header": "\"SpwfSAInterface.h\"",
            "easy-connect.network-interface-class": "SpwfSAInterface",
            "easy-connect.network-interface-name": "\"SpwfSAInterface\"",
            "easy-connect.wifi-ssid": "\"SSID\"",
            "easy-connect.wifi-password": "\"Password\"",
            "idw0xx1.tx": "D8",
            "idw0xx1.rx": "D2"
        }
    }
}
