{
    "config": {
        "button1": {
            "help": "Use BUTTON1 from PinNames.h by default",
            "value": "NC"
        }
    },
    "macros": ["MBEDTLS_USER_CONFIG_FILE=\"mbedtls_mbed_client_config.h\""],
    "target_overrides": {
        "*": {
            "drivers.uart-serial-rxbuf-size": 1024,
            "target.features_add": ["COMMON_PAL"],
            "platform.stdio-baud-rate": 115200,
            "platform.stdio-convert-newlines": true,
            "mbed-trace.enable": 0,
            "easy-connect.wifi-type": "\"RTW\"",
            "easy-connect.network-interface": "WIFI_RTW",
            "easy-connect.network-interface-header": "\"RTWInterface.h\"",
            "easy-connect.network-interface-class": "RTWInterface",
            "easy-connect.network-interface-name": "\"RTWInterface\"",
            "easy-connect.wifi-ssid": "\"SSID\"",
            "easy-connect.wifi-password": "\"Password\""
        }
    }
}
