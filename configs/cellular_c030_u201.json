{
    "config": {
    },
    "macros": ["MBEDTLS_USER_CONFIG_FILE=\"mbedtls_mbed_client_config.h\""],
    "target_overrides": {
        "*": {
            "target.features_add": ["LWIP", "COMMON_PAL"],
            "platform.stdio-baud-rate": 115200,
            "platform.stdio-convert-newlines":true,
            "lwip.ipv4-enabled": true,
            "lwip.ipv6-enabled": false,
            "mbed-trace.enable": 0,
            "easy-connect.network-interface": "CELLULAR_ONBOARD",
            "easy-connect.network-interface-header": "\"OnboardCellularInterface.h\"",
            "easy-connect.network-interface-class": "OnboardCellularInterface",
            "easy-connect.network-interface-name": "\"OnboardCellularInterface\""
        },
        "UBLOX_C030_U201": {
            "lwip.ppp-enabled": true,
            "ppp-cell-iface.apn-lookup": true
        }
    }
}
