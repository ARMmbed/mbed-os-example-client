{
    "config": {
        "mesh_radio_type": {
            "help": "options are ATMEL, MCR20, SPIRIT1, EFR32",
            "value": "ATMEL"
        },
        "wifi-tx": {
            "help": "TX pin for serial connection to external device",
            "value": "D1"
        },
        "wifi-rx": {
            "help": "RX pin for serial connection to external device",
            "value": "D0"
        }
    },
    "macros": ["MBEDTLS_USER_CONFIG_FILE=\"mbedtls_mbed_client_config.h\""],
    "target_overrides": {
        "*": {
            "target.features_add": ["NANOSTACK", "COMMON_PAL"],
            "nanostack.configuration": "lowpan_router",
            "platform.stdio-baud-rate": 115200,
            "platform.stdio-convert-newlines": true,
            "mbed-mesh-api.6lowpan-nd-channel-page": 0,
            "mbed-mesh-api.6lowpan-nd-channel": 12,
            "mbed-trace.enable": 0,
            "easy-connect.network-interface": "ETHERNET",
            "easy-connect.network-interface-header": "\"EthernetInterface.h\"",
            "easy-connect.network-interface-class": "EthernetInterface",
            "easy-connect.network-interface-name": "\"EthernetInterface\""
        },
        "DISCO_F413ZH": {
            "easy-connect.network-interface": "WIFI_ISM43362",
            "easy-connect.network-interface-header": "\"ISM43362Interface.h\"",
            "easy-connect.network-interface-class": "ISM43362Interface",
            "easy-connect.network-interface-name": "\"ISM43362Interface\"",
            "easy-connect.wifi-ssid": "\"SSID\"",
            "easy-connect.wifi-password": "\"Password\""

        },
        "DISCO_L475VG_IOT01A": {
            "easy-connect.network-interface": "WIFI_ISM43362",
            "easy-connect.network-interface-header": "\"ISM43362Interface.h\"",
            "easy-connect.network-interface-class": "ISM43362Interface",
            "easy-connect.network-interface-name": "\"ISM43362Interface\"",
            "easy-connect.wifi-ssid": "\"SSID\"",
            "easy-connect.wifi-password": "\"Password\""
        },
        "NUCLEO_F401RE": {
            "wifi-tx": "PA_11",
            "wifi-rx": "PA_12"
        },
        "NUCLEO_F411RE": {
            "wifi-tx": "PA_11",
            "wifi-rx": "PA_12"
        },
        "UBLOX_EVK_ODIN_W2": {
            "target.network-default-interface-type": "ETHERNET"
        },
        "UBLOX_C030_U201": {
            "lwip.ppp-enabled": true,
            "ppp-cell-iface.apn-lookup": true
        }
    }
}
